<%else%>
	<%= link_to content_tag(:span, "HOME"), root_path, :id=>"ultramenuNav-HOME", :class=>"", :target=>""%>
<%end%>
</li>
<li>
<% if @current_page == "flowers" %>
	<%= link_to content_tag(:span, "FLOWERS"), flowers_path, :id=>"ultramenuNav-FLOWERS", :class=>"selected first", :target=>""%>
<%else%>
	<%= link_to content_tag(:span, "FLOWERS"), flowers_path, :id=>"ultramenuNav-FLOWERS", :class=>"", :target=>""%>
<%end%>
</li>
<li>
<% if @current_page == "vegetables" %>
	<%= link_to content_tag(:span, "VEGETABLES"), vegetables_path, :id=>"ultramenuNav-VEGETABLES", :class=>"selected first", :target=>""%>
<%else%>
	<%= link_to content_tag(:span, "VEGETABLES"), vegetables_path, :id=>"ultramenuNav-VEGETABLES", :class=>"", :target=>""%>
<%end%>
</li>
<li>
<% if @current_page == "planters" %>
	<%= link_to content_tag(:span, "PLANTERS"), planters_path, :id=>"ultramenuNav-PLANTERS", :class=>"selected first", :target=>""%>
<%else%>
	<%= link_to content_tag(:span, "PLANTERS"), planters_path, :id=>"ultramenuNav-PLANTERS", :class=>"", :target=>""%>
<%end%>
</li>
<li>
<% if @current_page == "contact" %>
	<%= link_to content_tag(:span, "CONTACT"), contact_path, :id=>"ultramenuNav-CONTACT", :class=>"selected first", :target=>""%>
<%else%>
	<%= link_to content_tag(:span, "CONTACT"), contact_path, :id=>"ultramenuNav-CONTACT", :class=>"", :target=>""%>
<%end%>
</li>
</ul>
<script type="text/javascript" charset="utf-8">
        (function($) {

          $('#navPrime').ultramenu();
	
        })(jQuery);
</script>
</div>
</div>
<div id="content">

<div id="col_side">
	<hr class="divider">
	<div id="container_offer">
		<div id="container_content">
			<p></p>
		</div>
	</div>
</div>

<div id="col_main">
	<hr class="divider">
	<%= yield %>
</div>
<div class="clearing"></div>
</div>
</div>
<div id="footer">
<div id="footer-bannerRow"><div id="footer-centerAdjust"><div style="padding:30px 0px; text-align:center;">

</div></div></div>
<div id="footer-textRow"><div id="footer-centerAdjust"><div style="padding:30px 0px;">
<a href="">Help</a> | 
                <a href="">Newsletter</a> | 
                <a href="javascript:;" onclick="window.open ('','terms','menubar=0,scrollbars=1,resizable=1,width=800,height=550');">Terms Of Use</a> | 
                <a href="javascript:;" onclick="window.open ('','privacy','menubar=0,scrollbars=1,resizable=1,width=800,height=550');">Privacy Policy</a>
</div></div></div>
<script type="text/javascript">
      
      var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
      document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
      </script><script type="text/javascript">
      var pageTracker = _gat._getTracker("UA-856725-44");
      pageTracker._setDomainName("");
      pageTracker._initData();
      pageTracker._trackPageview();
      
      if (typeof(googlePageTrackerData) !== 'undefined') {
      
        var trans = googlePageTrackerData.trans;
  
        pageTracker._addTrans(
          trans.order_id,             // Order ID
          trans.affiliations,          // Affiliation
          trans.total,                // Total
          trans.tax,                  // Tax
          trans.shipping,             // Shipping
          trans.city,                 // City
          trans.state,                // State
          trans.country               // Country
        );
  
        var i, len;
        var items = googlePageTrackerData.items;
  
        for (i = 0, len = items.length; i < len; ++i) {
  
          var item = items[i];
  
          pageTracker._addItem(
            item.order_id,            // Order ID
            item.sku,                 // SKU
            item.product_name,        // Product Name 
            item.category,            // Category
            item.price,               // Price
            item.quantity             // Quantity
          );
        }
        
        pageTracker._trackTrans();      
      }
      
      </script>
</div>
</div>

</body></html>
