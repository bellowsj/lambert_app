</head>
<body marginheight="0" marginwidth="0" topmargin="0" leftmargin="0">
<div id="bg">
<div id="headerPIC"></div>
<div id="site">
<div id="header">
	<style type="text/css" title="css_main" media="screen">
	    	@import "/stylesheets/nav.css";
		</style>
<div id="headerLogin">
<script src="/javascripts/jquery.ultramenu.js" type="text/javascript" charset="utf-8"></script><style type="text/css" title="css_main" media="screen">
      @import "/stylesheets/jquery.ultramenu.css";
    </style>
<ul id="navPrime" class="ultramenu">
<li>
<% if @current_page == "home" %>
